{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "dbdiagram",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "table"
      }
    },
    "table": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_table"
        },
        {
          "type": "SYMBOL",
          "name": "table_name"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "body"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "keyword_table": {
      "type": "STRING",
      "value": "Table"
    },
    "table_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "body": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "column"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "constrant"
          }
        }
      ]
    },
    "column": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_]*"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_basic_type"
        },
        {
          "type": "SYMBOL",
          "name": "_type_length"
        }
      ]
    },
    "_basic_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "bigint"
        },
        {
          "type": "STRING",
          "value": "string"
        },
        {
          "type": "STRING",
          "value": "timestamp"
        },
        {
          "type": "STRING",
          "value": "int"
        },
        {
          "type": "STRING",
          "value": "bool"
        },
        {
          "type": "STRING",
          "value": "float"
        },
        {
          "type": "STRING",
          "value": "varchar"
        }
      ]
    },
    "_type_length": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "varchar"
            },
            {
              "type": "STRING",
              "value": "nvarchar"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "length"
        }
      ]
    },
    "length": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "parentheses",
          "content": {
            "type": "STRING",
            "value": "("
          }
        },
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "FIELD",
          "name": "parentheses",
          "content": {
            "type": "STRING",
            "value": ")"
          }
        }
      ]
    },
    "constrant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "unique"
        },
        {
          "type": "STRING",
          "value": "primary key"
        },
        {
          "type": "STRING",
          "value": "not null"
        },
        {
          "type": "STRING",
          "value": "null"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}